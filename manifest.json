{
"manifest_version":2,
"name":"DOM XSS detector",
"description":"This extension is not complete",
"version":"1.0",

"icons":{
    "32": "images/coin.png"
},

"permissions":[ 
    "cookies",
    "activeTab",
    "tabs",
    "scripting",
    "management",
    "https://raw.githubusercontent.com/adham-hashem/XSS-payloads/main/Payloads.txt",
    "https://github.com/yogsec/XSS-Payloads/blob/main/dom_based_xss_payloads.txt",
    "https://raw.githubusercontent.com/pgaijin66/XSS-Payloads/master/payload/payload.txt",
    "storage"

],

"content_scripts": [
    {
        "matches":["<all_urls>"],
        "js":[
        "javascript/purify.min.js",
        "javascript/tensorflow.js",
        "javascript/domDetector.js",
        "javascript/popUp.js",
        "javascript/getDataSets.js",
        "javascript/ai_model.js"
      ]
    }
],

"browser_specific_settings": {
  "gecko": {
    "id": "sample@example.com"
  }
},

"browser_action": {
  "default_icon":"images/coin.png",
  "default-title":"Extension",
  "default_popup": "interface.html"
}
}

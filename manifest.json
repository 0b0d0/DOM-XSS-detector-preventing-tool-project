{
"manifest_version":2,
"name":"DOM XSS detector",
"description":"This extension is not complete",
"version":"1.0",

"icons":{
    "32": "coin.png"
},

"permissions":[ 
    "cookies",
    "active-tab",
    "tabs",
    "scripting",
    "https://github.com/adham-hashem/XSS-payloads/blob/main/Payloads.txt"
],

"content_scripts": [
    {
        "matches":["<all_urls>"],
        "js":[
        "domDetector.js", 
        "popUp.js",
        "domXSSpayload.js"
      ]
    }
],

"browser_specific_settings": {
  "gecko": {
    "id": "sample@example.com"
  }
},

"browser_action": {
  "default_icon":"coin.png",
  "defualt-title":"Extension",
  "default_popup": "interface.html"
}

}


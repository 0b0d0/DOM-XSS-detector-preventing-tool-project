{
"manifest_version":2,
"name":"DOM XSS detector",
"description":"This extension is not complete",
"version":"1.0",

"icons":{
    "32": "coin.png"
},

"permissions":[ 
    "cookies",
    "activeTab",
    "tabs",
    "scripting",
    "management"

],

"content_scripts": [
    {
        "matches":["<all_urls>"],
        "js":[
        "domDetector.js",
        "popUp.js"
      ]
    }
],

"background":{
"scripts":["extensionStatusLogger.js"],
"persistent": true
},

"browser_specific_settings": {
  "gecko": {
    "id": "sample@example.com"
  }
},

"browser_action": {
  "default_icon":"coin.png",
  "defualt-title":"Extension",
  "default_popup": "interface.html"
}
}

